<div class="schedule-week-holder" weekno="[@week_no]">
    <table class="schedule_table" cellspacing="0" cellpadding="0" border="0" width="100%">
        <thead>
            <tr>
                <th>[@week_title]</th>
                <th class="schedule-header " height="24">[@day_1_title]</th>
                <th class="schedule-header " height="24">[@day_2_title]</th>
                <th class="schedule-header " height="24">[@day_3_title]</th>
                <th class="schedule-header " height="24">[@day_4_title]</th>
                <th class="schedule-header " height="24">[@day_5_title]</th>
                <th class="schedule-header " height="24">[@day_6_title]</th>
                <th class="schedule-header " height="24">[@day_7_title]</th>
            </tr>
       </thead>
       <tbody>
            <tr>
                <td width="12.5%" valign="top">[@time_line]</td>
                <td class="[@day_1_class]" width="12.5%" valign="top">[@day_1_list]</td>
                <td class="[@day_2_class]" width="12.5%" valign="top">[@day_2_list]</td>
                <td class="[@day_3_class]" width="12.5%" valign="top">[@day_3_list]</td>
                <td class="[@day_4_class]" width="12.5%" valign="top">[@day_4_list]</td>
                <td class="[@day_5_class]" width="12.5%" valign="top">[@day_5_list]</td>
                <td class="[@day_6_class]" width="12.5%" valign="top">[@day_6_list]</td>
                <td class="[@day_7_class]" width="12.5%" valign="top">[@day_7_list]</td>
            </tr>
        </tbody>
    </table>
</div>
