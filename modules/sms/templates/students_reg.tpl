<page class="page" style="page-break-after:always">
	<page_header class="page_header">
    	<img src="attachs/img/header.jpg" width="100%" />
    </page_header>
    <h2 align="center">[#reg_report] [#year] [@year]</h2>
    <!--<h2 align="center">[@school_name]</h2>-->
    <h3 align="center">[@title]</h3>
    <table width="100%" class="result">
        <thead>
            <tr>
                <th rowspan="2" width="20">[#ser]</th>
                <th rowspan="2">[#name]</th>
                <th width="100"colspan="3">[#birth_date]</th>
                <th width="100" colspan="3">[#age_in_first_oct]</th>
                <th width="60" rowspan="2">[#nationality_ar]</th>
                <th width="60" rowspan="2">[#religion]</th>
                <th width="60" rowspan="2">[#reg_stat]</th>
                <th rowspan="2">[#father_job]</th>
                <th rowspan="2">[#tel]</th>
                <th rowspan="2">[#father_name_ar]</th>
                <th rowspan="2">[#address_ar]</th>
                <th width="60" rowspan="2">[#reg_no]</th>
                <th width="60" rowspan="2">[#birth_city_ar]</th>
            </tr>
            <tr>
                <th width="20">[#day]</th>
                <th width="20">[#month]</th>
                <th width="30">[#year]</th>
                <th width="20">[#day]</th>
                <th width="20">[#month]</th>
                <th width="40">[#year]</th>
            </tr>
        </thead>
        <tbody>
            [@trs]
        </tbody>
    </table>
    
<div class="ui-widget-content ui-corner-all" style="margin:10px 20px 20px">
        <table width="100%">
            <tr>
                <td width="50%" align="center">[#student_affaire]</td>
                <td width="50%" align="center">[#school_principal]</td>
            </tr>
        </table>
    </div>
</page>