<div class="session-holder-[@id] scope" >
	[@toolbox]
	<table width="100%" class="layout">
    	<tr>
        	[@summary_td]
            <td valign="top">
            	<div class="ui-corner-all ui-state-highlight">
                	<h2><text class="holder-service-[@service_id]">[@material_name]</text></h2>
                	<ul style="list-style:none; padding:0; margin:0">
                    	<li><b>[#date]:</b> [@date]</li>
                        <li><b>[#prof]:</b> [@prof_name]</li>
                        <li><b>[#students]:</b> [@con_name]</li>
                        <li><b>[#hall]:</b> [@hall_name]</li>
                    </ul>
                </div>
				<div class="accordion">[@accordion1]</div>                
            </td>
        </tr>
    </table>
</div>
